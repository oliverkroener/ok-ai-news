{
    "name": "oliverkroener/ok-ai-news",
    "type": "typo3-cms-extension",
    "description": "TYPO3 extension to create news records from webhook reactions with image support",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Oliver Kroener",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "typo3/cms-core": "^12.4 || ^13.4",
        "typo3/cms-reactions": "^12.4 || ^13.4",
        "georgringer/news": "^11.0 || ^12.0"
    },
    "autoload": {
        "psr-4": {
            "OliverKroener\\OkAiNews\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "ok_ai_news"
        }
    }
}
